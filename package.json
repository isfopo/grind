{
  "name": "grind",
  "displayName": "Grind",
  "publisher": "isfopo",
  "description": "",
  "version": "0.0.0",
  "icon": "assets/icon.png",
  "type": "module",
  "engines": {
    "vscode": "^1.91.0"
  },
  "bugs": {
    "url": "https://github.com/isfopo/grind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isfopo/grind"
  },
  "categories": [
    "Other"
  ],
  "browser": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "grind.add",
        "title": "Add",
        "icon": "$(add)"
      },
      {
        "command": "grind.edit",
        "title": "Edit",
        "icon": "$(edit)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "grind.refresh",
          "group": "navigation",
          "when": "view == grind-sidebar || view == grind-explorer"
        }
      ],
      "view/item/context": [
        {
          "command": "grind.add",
          "when": "viewItem == file || viewItem == group-has-presets || viewItem == group",
          "group": "inline"
        },
        {
          "command": "grind.edit",
          "when": "viewItem == keyValue-string",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "grind-sidebar-view",
          "title": "grind",
          "icon": "assets/icon.svg"
        }
      ]
    },
    "views": {
      "grind-sidebar-view": [
        {
          "id": "grind-sidebar",
          "name": "grind",
          "icon": "media/icon.svg"
        }
      ],
      "explorer": [
        {
          "id": "grind-explorer",
          "name": "grind",
          "icon": "media/icon.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile-web",
    "compile-web": "vite build",
    "watch-web": "vite build"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.91.0",
    "eslint": "^9.7.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  },
  "dependencies": {
    "proposal-temporal": "^0.9.0"
  }
}